{
	"name": "dasred/translation",
	"homepage": "https://github.com/DasRed/translation",

	"prefer-stable": true,
	"minimum-stability": "dev",

	"require": {
		"php": ">=5.5",
		"zendframework/zend-config": "~2.5",
		"zendframework/zend-log": "~2.5",
		"zendframework/zend-servicemanager": "~2.5",
		"dasred/zend-console": "@stable",
		"dasred/bb-code-parser": "@stable"
	},

	"require-dev": {
		"phpoffice/phpexcel": "~1.8"
	},

	"autoload": {
		"psr-4": {
			"DasRed\\Translation\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"DasRedTest\\Translation\\": "test/"
		}
	},

	"bin": [
		"bin/translation"
	],

	"suggest": {
		"phpoffice/phpexcel": "~1.8 for using shell scripts export and import."
	}
}